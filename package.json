    "test": "c8 mocha build/test",
    "codecov": "c8 report --reporter=json && codecov -f coverage/*.json"
  },
  "devDependencies": {
    "@types/mocha": "^7.0.0",
    "@types/node": "^11.12.3",
    "@types/npm-packlist": "^1.1.1",
    "@types/once": "^1.4.0",
    "@types/rimraf": "^2.0.2",
    "@types/tar": "^4.0.0",
    "@types/tmp": "0.1.0",
    "c8": "^7.0.0",
    "codecov": "^3.3.0",
    "gts": "^1.0.0",
    "mocha": "^7.0.0",
    "typescript": "~3.7.0"
  },
  "dependencies": {
    "execa": "^3.3.0",
    "npm-packlist": "^2.0.0",
    "once": "^1.4.0",
    "rimraf": "^3.0.0",
    "tar": "^6.0.0",
    "tmp": "0.2.0"
  },
  "engines": {
    "node": ">=10.x"
  },
  "c8": {
    "exclude": [
      "build/test"
    ]
